#!/bin/bash

docker stack rm $1


echo "###################################################################################"
echo  "###################  VOLUMES   ###################################################"
echo "###################################################################################"
docker volume prune -f


echo "###################################################################################"
echo  "###################  NETWORKS  ###################################################"
echo "###################################################################################"


docker network prune -f
echo "###################################################################################"
echo  "###################  CONTAINERS   ################################################"
echo "###################################################################################"
docker rm $(docker ps -a -f status=exited -q)
